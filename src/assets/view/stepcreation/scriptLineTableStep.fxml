<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" fx:id="anchroPaneScript" maxHeight="75.0"
            minHeight="50.0"
            prefHeight="50.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.tablestep.ScriptLineTableStepController">
    <children>
        <GridPane fx:id="gridPaneScript" alignment="CENTER" gridLinesVisible="true" prefHeight="400.0" prefWidth="600.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="70.0" minWidth="10.0" prefWidth="70.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="40.0" minWidth="10.0"
                                   prefWidth="40.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="45.0" minWidth="10.0"
                                   prefWidth="45.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="45.0" minWidth="10.0"
                                   prefWidth="45.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="26.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="135.0" minWidth="10.0" prefWidth="135.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="60.0" minWidth="10.0" prefWidth="60.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <AnchorPane GridPane.columnIndex="9">
                    <children>
                        <Label fx:id="labelCommentStep" alignment="TOP_LEFT" prefHeight="50.0" prefWidth="89.0"
                               wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </Label>
                    </children>
                </AnchorPane>
                <ListView fx:id="listRequirements" prefHeight="200.0" prefWidth="200.0"
                          style="-fx-background-color: transparent;" GridPane.columnIndex="7"/>
                <ImageView fx:id="paramVerif" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true"
                           visible="false" GridPane.columnIndex="5" GridPane.halignment="CENTER"
                           GridPane.valignment="CENTER"/>
                <ImageView fx:id="paramAction" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true"
                           visible="false" GridPane.columnIndex="3" GridPane.halignment="CENTER"
                           GridPane.valignment="CENTER"/>
                <Label fx:id="labelID" contentDisplay="CENTER" GridPane.columnIndex="1"/>
                <HBox alignment="CENTER" spacing="5.0">
                    <children>
                        <ImageView fx:id="imageViewTrash" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                   preserveRatio="true" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
                            <GridPane.margin>
                                <Insets left="3.0"/>
                            </GridPane.margin>
                        </ImageView>
                        <VBox alignment="CENTER" spacing="16.0">
                            <children>
                                <ImageView fx:id="imageUp" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                           preserveRatio="true"/>
                                <ImageView fx:id="imageDown" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                           preserveRatio="true"/>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <AnchorPane fx:id="paneVerif" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="5"/>
                <AnchorPane fx:id="paneAction" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3"/>
                <Label fx:id="labelAction" contentDisplay="TOP" wrapText="true" GridPane.columnIndex="3"/>
                <Label fx:id="labelVerif" wrapText="true" GridPane.columnIndex="5"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
