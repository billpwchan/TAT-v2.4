<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" fx:id="anchorPaneTestExecution" prefHeight="768.0"
            prefWidth="1024.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.tabtestexecution.TabTestCampaignExecutionRepositoryBaselineController">
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints/>
            </rowConstraints>
            <children>
                <TreeTableView fx:id="campaignBaselineAndExecution" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TreeTableView>
                <GridPane style="-fx-background-color: #ffee58;">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Configurations and executions">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets left="20.0"/>
                            </GridPane.margin>
                        </Text>
                        <HBox alignment="CENTER_RIGHT" prefHeight="89.0" prefWidth="477.0" spacing="5.0"
                              GridPane.columnIndex="1">
                            <children>
                                <Button fx:id="baselineButton" alignment="CENTER" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" prefHeight="32.0" prefWidth="100.0"
                                        style="-fx-background-color: transparent;" text="Baseline">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/flag-triangle.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="runButton" maxHeight="-Infinity" maxWidth="-Infinity"
                                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                        prefHeight="32.0" prefWidth="100.0" style="-fx-background-color: transparent;"
                                        text="Execute">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/run.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="buttonReport" maxHeight="-Infinity" maxWidth="-Infinity"
                                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                        prefHeight="32.0" prefWidth="100.0" style="-fx-background-color: transparent;"
                                        text="Report">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/book-open.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                </Button>
                                <Button fx:id="buttonDelete" maxHeight="-Infinity" maxWidth="-Infinity"
                                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                        prefHeight="32.0" prefWidth="100.0" style="-fx-background-color: transparent;"
                                        text="Delete">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/delete.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
            </children>
            <padding>
                <Insets bottom="10.0"/>
            </padding>
        </GridPane>
    </children>
</AnchorPane>
