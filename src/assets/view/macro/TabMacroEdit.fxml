<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" fx:id="anchorPanelEditMacro" prefHeight="768.0"
            prefWidth="1024.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.macro.TabMacroEditController">
    <children>
        <GridPane fx:id="gridPaneMacroEdit" prefHeight="768.0" prefWidth="1024.0" vgap="10.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>

                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="200.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES"/>
                <RowConstraints vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="buttonValid" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="32.0"
                        prefWidth="100.0" text="Save" GridPane.halignment="CENTER" GridPane.rowIndex="4"
                        GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <font>
                        <Font size="14.0"/>
                    </font>
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@/assets/images/content-save.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="3">
                    <children>
                        <GridPane fx:id="gridPaneTableAction" maxHeight="50.0" minHeight="50.0" prefHeight="408.0"
                                  prefWidth="1014.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="75.0" minHeight="75.0" prefHeight="75.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="5.0" minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="30.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                        </GridPane>
                    </children>
                </AnchorPane>
                <GridPane GridPane.rowIndex="2">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Text fx:id="labelActionTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Actions">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </Text>
                        <HBox alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="1">
                            <children>
                                <Button fx:id="buttonAddAction" alignment="CENTER" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" prefHeight="32.0" prefWidth="100.0" text="Add script">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/script.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </GridPane>
                <GridPane fx:id="gridPaneLabelCaseEdit" GridPane.rowIndex="1">
                    <columnConstraints>
                        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0"
                                           prefWidth="100.0"/>
                        <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0"
                                           prefWidth="160.0"/>
                        <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0"
                                           prefWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="100.0" prefWidth="250.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="250.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                        <RowConstraints/>
                    </rowConstraints>
                    <children>
                        <Label fx:id="labelMacroNameEdit" text="Macro name"/>
                        <Label fx:id="labelMacroEditionDate" text="Edition date" GridPane.rowIndex="3"/>
                        <Label fx:id="labelMacroCreationDate" text="Creation date" GridPane.rowIndex="2"/>
                        <Label fx:id="labelMacroVersion" text="Macro version" GridPane.rowIndex="1"/>
                        <Label fx:id="labelMacroObjectives" text="Macro description" GridPane.columnIndex="3"/>

                        <!-- 1st column of the assets.view textfield in testcase -->
                        <TextField fx:id="jtextfieldMacroNameEdit" maxWidth="150.0" GridPane.columnIndex="1"/>
                        <TextField fx:id="jtextfieldMacroEdit" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                        <TextField fx:id="jtextfieldMacroCreationDateEdit" editable="false" maxWidth="150.0"
                                   GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                        <TextField fx:id="jtextfieldTypeMacroEditionDateEdit" editable="false" maxWidth="150.0"
                                   GridPane.columnIndex="1" GridPane.rowIndex="3"/>

                        <TextArea fx:id="jtextareaObjectivesMacroEdit" prefHeight="200.0" prefWidth="200.0"
                                  wrapText="true" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.rowSpan="3"/>
                        <ScrollPane fx:id="scrollPanePreview" prefHeight="200.0" prefWidth="200.0"
                                    GridPane.columnIndex="5" GridPane.rowIndex="1" GridPane.rowSpan="5"/>
                        <Label fx:id="labelIsStimuli" text="Stimuli" GridPane.rowIndex="4"/>
                        <CheckBox fx:id="stimuliCheckBoxEdit" mnemonicParsing="false" GridPane.columnIndex="1"
                                  GridPane.halignment="CENTER" GridPane.rowIndex="4">
                            <GridPane.margin>
                                <Insets right="15.0"/>
                            </GridPane.margin>
                        </CheckBox>
                        <AnchorPane fx:id="anchorHeader" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0"
                                    prefWidth="200.0" GridPane.columnIndex="5" GridPane.valignment="BOTTOM"/>
                    </children>
                </GridPane>


                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Macro information">

                    <font>
                        <Font name="System Bold" size="16.0"/>
                    </font>
                </Text>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Macro information">
                    <font>
                        <Font name="System Bold" size="16.0"/>
                    </font>
                </Text>
                <Text fx:id="labelPreview" strokeType="OUTSIDE" strokeWidth="0.0" text="Macro Preview">
                    <font>
                        <Font size="16.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets left="535.0"/>
                    </GridPane.margin>
                </Text>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </GridPane>
    </children>
</AnchorPane>
