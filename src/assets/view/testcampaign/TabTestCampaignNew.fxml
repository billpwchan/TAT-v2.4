                    <children>
                        <TableView fx:id="TableViewTestCasesAdded" prefHeight="200.0" prefWidth="200.0"
                                   GridPane.rowSpan="7"/>
                    </children>
                </GridPane>
                <Text fx:id="textTestCaseAddCampaign1" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Test campaign information">
                    <font>
                        <Font name="System Bold" size="16.0"/>
                    </font>
                </Text>
