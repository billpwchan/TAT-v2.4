                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets left="10.0"/>
                            </GridPane.margin>
                        </Text>
                        <HBox alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="1">
                            <children>
                                <Button fx:id="buttonUp" alignment="CENTER" contentDisplay="RIGHT" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" prefHeight="32.0" prefWidth="110.0">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true" rotate="180.0">
                                            <image>
                                                <Image url="@/assets/images/arrowDown.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="buttonDown" alignment="CENTER" contentDisplay="RIGHT"
                                        maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                        minWidth="-Infinity" mnemonicParsing="false" prefHeight="32.0"
                                        prefWidth="110.0">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/arrowDown.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="buttonAddCase" alignment="CENTER" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" prefHeight="32.0" prefWidth="110.0" text="Add case">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/plus.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="buttonDeleteCase" alignment="CENTER" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" prefHeight="32.0" prefWidth="110.0" text="Delete case">
                                    <graphic>
                                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image url="@/assets/images/delete.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </GridPane>
                <Button fx:id="buttonValidCampaign" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
