                        <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label fx:id="labelReferenceAddCampaign" text="Campaign ID"/>
                        <TextField fx:id="jtextfieldReferenceAddCampaign" maxWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.halignment="LEFT"/>

                        <Label fx:id="labelVersionAddCampaign" text="Version" GridPane.rowIndex="1"/>
                        <TextField fx:id="jtextfieldVersionAddCampaign" disable="true" editable="false" maxWidth="150.0"
                                   opacity="0.7" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

                        <Label fx:id="labelSystemAddCampaign" text="System" GridPane.rowIndex="2"/>
                        <TextField fx:id="jtextfieldSystemAddCampaign" maxWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="2"/>

                        <Label fx:id="labelWriterAddCampaign" text="Writer" GridPane.rowIndex="3"/>
                        <TextField fx:id="jtextfieldWriterAddCampaign" maxWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="3"/>

                        <Label fx:id="labelCreationDateAddCampaign" text="Creation date" GridPane.columnIndex="3"/>
                        <TextField fx:id="jtextfieldCreationDateAddCampaign" disable="true" editable="false"
