                        <Font size="14.0"/>
                    </font>
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="/assets/images/content-save.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
