                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Text fx:id="textTestCaseAddCampaign" strokeType="OUTSIDE" strokeWidth="0.0" text="Test cases :"
