                        <Label fx:id="labelEditionDateAddCampaign" text="Edition date" GridPane.columnIndex="3"
                               GridPane.rowIndex="1"/>
                        <TextField fx:id="jtextfieldEditionDateAddCampaign" disable="true" editable="false"
                                   maxWidth="150.0" opacity="0.7" GridPane.columnIndex="4" GridPane.rowIndex="1"/>

                        <Label fx:id="labelSUTReleaseAddCampaign" text="SUT Release" GridPane.columnIndex="3"
                               GridPane.rowIndex="2"/>
                        <TextField fx:id="jtextfieldSUTReleaseAddCampaign" maxWidth="150.0" GridPane.columnIndex="4"
                                   GridPane.rowIndex="2"/>

                        <Label fx:id="labelWriterMailAddCampaign" text="Writer Email" GridPane.columnIndex="3"
                               GridPane.rowIndex="3"/>
                        <TextField fx:id="jtextfieldWriterMailAddCampaign" maxWidth="150.0" GridPane.columnIndex="4"
                                   GridPane.rowIndex="3"/>

                        <Label fx:id="labelNumberCasesAddCampaign" text="Numbe of Cases" GridPane.columnIndex="6"/>
                        <TextField fx:id="jtextfieldCasesNumberAddCampaign" disable="true" maxWidth="150.0"
                                   opacity="0.7" GridPane.columnIndex="7"/>

                        <Label fx:id="labelRegressionThreadAddCampaign" text="Regression Thread"
                               GridPane.columnIndex="6" GridPane.rowIndex="1"/>
                        <CheckBox fx:id="checkboxRegressionThreadAddCampaign" mnemonicParsing="false"
                                  GridPane.columnIndex="7" GridPane.halignment="LEFT" GridPane.rowIndex="1"
                                  GridPane.valignment="CENTER"/>

                        <Label fx:id="labelCommentsAddCampaign" text="Internal Comments" GridPane.columnIndex="11"
                               GridPane.halignment="LEFT" GridPane.valignment="CENTER"/>
                        <TextArea fx:id="textareaCommentsAddCampaign" maxHeight="-Infinity" maxWidth="240.0"
                                  wrapText="true" GridPane.columnIndex="11" GridPane.rowIndex="1" GridPane.rowSpan="5"/>

                        <ListView fx:id="listViewPCRCaseNew" prefHeight="200.0" prefWidth="200.0"
                                  GridPane.columnIndex="9" GridPane.rowIndex="1" GridPane.rowSpan="5"/>
                        <Label fx:id="labelListViewCaseNew1" text="Requirements" GridPane.columnIndex="9"/>
                    </children>
                </GridPane>
                <GridPane GridPane.rowIndex="3">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
